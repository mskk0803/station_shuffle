<div class="flex flex-col items-center w-full space-y-4 mt-6">
  <% if likes.empty? %>
    <p>いいねした投稿はありません</p>
  <% else %>
    <% likes.each do |like| %>   
      <div class="w-full max-w-2xl">
          <%= render partial: "shared/other_post", locals: { post: like } %>  
      </div>
    <% end %>
  <% end %>
</div>